  </PropertyGroup>
  <ItemGroup>
    <InvokedTargetsList Include="$(OBJ_PATH)\$(O)\PersoSimVirtualReader.inf">
      <Pass>1</Pass>
      <Condition>
      </Condition>
    </InvokedTargetsList>
  </ItemGroup>
